<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="root" prefHeight="436.0" prefWidth="980.0" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ace.ucv.controller.table.StudentTableController">
    <VBox prefHeight="436.0" prefWidth="980.0">
        <HBox prefHeight="438.0" prefWidth="980.0">
            <TableView fx:id="tbStudentData" prefHeight="436.0" prefWidth="979.0" stylesheets="@../css/style.css">
                <columns>
                    <TableColumn fx:id="studentId" minWidth="0.0" prefWidth="33.0" text="ID"/>
                    <TableColumn fx:id="firstName" prefWidth="673.0" text="NAME"/>
                    <TableColumn fx:id="lastName" prefWidth="75.0" text="SURNAME"/>
                    <TableColumn fx:id="genre" minWidth="0.0" prefWidth="33.0" text="GENRE"/>
                    <TableColumn fx:id="phone" minWidth="0.0" prefWidth="33.0" text="PHONE"/>
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                </columnResizePolicy>
            </TableView>
        </HBox>
    </VBox>
</AnchorPane>
