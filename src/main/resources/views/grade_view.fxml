<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane fx:id="root" prefHeight="436.0" prefWidth="980.0" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ace.ucv.controller.table.GradeTableController">
    <VBox prefHeight="436.0" prefWidth="980.0">
        <HBox prefHeight="438.0" prefWidth="980.0">
            <TableView fx:id="tbGradeData" prefHeight="436.0" prefWidth="979.0" stylesheets="@../css/style.css">
                <columns>
                    <TableColumn fx:id="studentName" minWidth="0.0" prefWidth="33.0" text="STUDENT NAME"/>
                    <TableColumn fx:id="studentSurname" prefWidth="673.0" text="STUDENT SURNAME"/>
                    <TableColumn fx:id="discipline" prefWidth="75.0" text="DISCIPLINE"/>
                    <TableColumn fx:id="grade" minWidth="0.0" prefWidth="33.0" text="GRADE"/>
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                </columnResizePolicy>
            </TableView>
        </HBox>
    </VBox>
</AnchorPane>
